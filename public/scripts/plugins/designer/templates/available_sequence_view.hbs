{{#each sequence}}
<div class="designer-available-sequence-features-entireseq">
<h6>{{name}}</h6>

{{#unless hidden}}

<div class="designer-available-sequence-feature-entireseq" data-entireSeq-id="{{id}}">Entire Sequence</div>
{{/unless}}

</div>
{{/each}}
<div class="designer-available-sequence-features">
  {{#each features}}
    <div class="designer-available-sequence-feature" data-feature-id="{{id}}">
      {{name}}
    </div>
  {{/each}}
</div>