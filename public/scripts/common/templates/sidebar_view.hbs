{{#each tabs}}
  <div>
    <a href='#{{../sidebarName}}-{{name}}' class='sidebar-tab-link' title="{{title}}" data-placement="right">
      <i class="glyphicon glyphicon-{{icon}}"></i>&nbsp;
      <span>{{title}}</span>
    </a>
    <div id='{{../sidebarName}}-{{name}}-tab' class='sidebar-tab{{#if maxHeighted}} max-heighted{{/if}}'>
      <span class="sidebar-popout-title">{{title}}</span>
      <div class="sidebar-tab-outlet"></div>
    </div>
  </div>
{{/each}}