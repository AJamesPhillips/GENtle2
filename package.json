{
  "name": "GENtle",
  "description": "GENtle - The Browser Based, Open Source, DNA Assembly App",
  "homepage": "http://gentledna.org/",
  "version": "3.0.0-alpha.1",
  "engines": {
    "node": "^0.11"
  },
  "browser": {
    "jquery.mixed": "./public/scripts/common/lib/jquery.mixed.js",
    "backbone.mixed": "./public/scripts/common/lib/backbone.mixed.js",
    "underscore.mixed": "./public/scripts/common/lib/underscore.mixed.js",
    "gentle": "./public/scripts/common/models/gentle.js",
    "bootstrap": "./public/vendor/bootstrap/js/bootstrap.js",
    "handlebars.mixed": "./public/scripts/common/lib/handlebars.mixed.js"
  },
  "browserify": {
    "transform": [
      ["hbsfy", {
          "compiler": "require('handlebars.mixed')"
      }],
      "browserify-shim",
      "deamdify"
    ]
  },
  "browserify-shim": {
    "backbone-deep-model": {
      "depends": [
        "underscore",
        "backbone"
      ]
    }
  },
  "dependencies": {
    "backbone": "~1.1.2",
    "backbone-deep-model": "~0.11.0",
    "backbone.layoutmanager": "~0.9.5",
    "backbone.localstorage": "~1.1.14",
    "browserify": "^7.1.0",
    "browserify-shim": "^3.8.1",
    "filesaver.js": "~0.1.1",
    "grunt": "~0.4.1",
    "grunt-cli": "0.1.9",
    "handlebars": "~2.0.0",
    "hbsfy": "^2.2.1",
    "jquery": "~2.1.3",
    "jquery-ui": "~1.10.5",
    "jquery-ui-touch-punch": "~0.2.3",
    "koa": "^0.6.1",
    "koa-favicon": "^1.1.0",
    "koa-request": "^1.0.0",
    "koa-router": "^3.1.4",
    "koa-static": "^1.4.5",
    "koa-views": "^2.0.3",
    "q": "~1.1.2",
    "then-jade": "^2.0.0",
    "underscore": "^1.6.0",
    "underscore-deep-extend": "0.0.5"
  },
  "devDependencies": {
    "grunt-contrib-watch": "~0.5.3",
    "grunt-contrib-yuidoc": "~0.5.0",
    "grunt-contrib-sass": "^0.7.3",
    "benchmark": "~1.0.0",
    "amdefine": "~0.1.0",
    "grunt-notify": "0.2.20",
    "sassyjson": "^1.1.8",
    "grunt-contrib-compass": "^0.8.0",
    "deamdify": "Automattic/deamdify.git#master"
  },
  "scripts": {
    "start": "nf start webdev,doc",
    "postinstall": "./node_modules/grunt-cli/bin/grunt js"
  }
}
